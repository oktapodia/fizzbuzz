{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "airbnb",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "no-console": "off",
    "class-methods-use-this": "off",
    "prefer-arrow-callback": 0,
    "new-cap": 0,
    "no-param-reassign": [
      1,
      {
        "props": false
      }
    ],
    "no-unused-vars": [
      1,
      {
        "args": "none"
      }
    ]
  }
}
